{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "mvn",
			"args": [
				"clean",
				"package"
			]
		},
		{
			"label": "build-debug",
			"type": "shell",
			"command": "mvn",
			"args": [
				"package",
				"-Dskip-vsce=true",
				"-DskipTests"
			]
		}
	]
}